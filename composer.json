{
    "name": "da/di-bundle",
    "type": "symfony-bundle",
    "description": "Bundle allowing to add parameters to the dependency injection",
    "keywords": ["di", "bundle", "da", "dependency", "injection", "parameters", "new", "add"],
    "homepage": "http://github.com/Gnucki/DaDiBundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Thomas Prelot",
            "email": "tprelot@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.3.3",
        "symfony/dependency-injection" : ">=2.1,<2.4"
    },
    "require-dev": {
        "phpunit/phpunit": "3.7.*"
    },
    "autoload": {
        "psr-0": { "Da\\DiBundle": "" }
    },
    "minimum-stability": "dev",
    "target-dir": "Da/DiBundle"
}